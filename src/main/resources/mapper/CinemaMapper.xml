<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yupi.project.mapper.CinemaMapper">

    <resultMap id="BaseResultMap" type="com.yupi.project.model.entity.Cinema">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="name" column="name" jdbcType="VARCHAR"/>
            <result property="adress" column="adress" jdbcType="VARCHAR"/>
            <result property="price" column="price" jdbcType="INTEGER"/>
            <result property="distance" column="distance" jdbcType="VARCHAR"/>
            <result property="available_date" column="available_date" jdbcType="DATE"/>
            <result property="movieid" column="movieid" jdbcType="INTEGER"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,name,adress,
        price,distance,available_date,
        movieid
    </sql>
</mapper>
